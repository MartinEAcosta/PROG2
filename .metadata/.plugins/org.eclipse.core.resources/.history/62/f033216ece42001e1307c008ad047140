package TP2.Agenda;

import java.time.LocalDate;
import java.time.LocalTime;

public class Main {
	public static void main(String[] args) {
		
		Persona martin = new Persona("martin","martin@gmail,com",249455555);
		Persona juan = new Persona("juan","juancito@gmail,com",249455555);
		Persona carlo = new Persona("carlo","carlito@gmail,com",249455555);
		
		Reunion rr = new Reunion("Plaza","CG NEW",1.5,LocalDate.of(2023,9,24),LocalTime.of(20,30));
		
		Agenda ag = new Agenda();
		
		rr.addPersona(martin);
		rr.addPersona(carlo);
		rr.addPersona(juan);
		ag.addReunion(rr);
		
	}
}
