package TP2.Agenda;

import java.time.LocalDate;
import java.time.LocalTime;
import java.time.Month;

public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Reunion a = new Reunion("Tandil", "Juntada a estudiar prog",LocalDate.of(2023,Month.AUGUST, 25), LocalTime.of(20,30),LocalTime.of(21,30));
		Persona Juan = new Persona("Juan","juan@gmail.com", 2);
		a.addPersonas(Juan);
		System.out.println(a.getPersonas());
		
	}

}
